{% extends '::base.html.twig' %}

{% block body -%}

  	<h3>{{ 'export.file_generated' | trans }}</h3>

  	<h3><a href="{{ asset(urlCSVRelativeToWeb) ~ csvName }}">{{ 'export.file_download' | trans }}</a></h3>

    <h3>{{ 'export.files_list' | trans }}</h3>

    <h4>{{ 'export.files_count' | trans({'%nbFile%': nbFile}) }}</h4>

  	{# Files export CSV list  #}
		{% for fileList in fileList %}
			<h5><a href="{{ asset(urlCSVRelativeToWeb) ~ fileList }}">{{ fileList }}</a></h5>
    {% endfor %}

{% endblock %}
