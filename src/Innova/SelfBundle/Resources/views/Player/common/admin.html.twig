{% if is_granted('ROLE_ADMIN') %}
    <div id="pick-a-questionaire" class="row text-center">
        {% for i, questionnaire in questionnaires %}
            <a class="btn btn-default btn-xs" href="{{ path('questionnaire_pick', { testId: test.id, questionnaireId: questionnaire.id }) }}">{{ questionnaire.theme}}</a>
        {% endfor %}
    </div>
{% endif %}
