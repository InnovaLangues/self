{% extends '::base.html.twig' %}

{% block body -%}

<h3>Liste des utilisateurs actifs </h3>


<table class="table table-striped">
    <thead>
        <tr>
            <th>Login</th>
            <th  class="text-right">Admin</th>
        </tr>
    </thead>
    <tbody>
        {% for connectedUser in connectedUsers %}
            <tr>
                <td>
                    {{ connectedUser[0] }}            
                </td>
                <td class="text-right">
                    {{ connectedUser[1] }}            
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>

{% endblock %}
