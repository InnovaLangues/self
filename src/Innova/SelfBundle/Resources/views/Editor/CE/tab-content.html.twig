<div class="tab-content">
    {% for i, subquestion in question.subquestions %}
        <div class="tab-pane {% if i == 0 %} active {% endif %}" id="tab-{{ i }}" data-subquestion-id="{{ subquestion.id }}">
            <div class="row">
                {# CONSIGNE #}
                {% include "InnovaSelfBundle:Editor/CE:consigne.html.twig" %}
                {# PROPOSITIONS #}
                {% include "InnovaSelfBundle:Editor/CE:propositions.html.twig" %}
            </div>
            {% include "InnovaSelfBundle:Editor/common:countRightAnswer.html.twig" %}
        </div>
    {% endfor %}
</div>